# Defaults for flamehaze initscript
# sourced by /etc/init.d/flamehaze
# installed at /etc/default/flamehaze by the maintainer scripts

export FLAMEHAZE_TMP_SRC_FILE=/dev/shm/flamehaze-tmp
export FLAMEHAZE_TMP_IMG_FILE=/dev/shm/target.img
export FLAMEHAZE_CONFIG_FILE=/etc/flamehaze.ini
export FLAMEHAZE_PID_FILE=/var/run/flamehaze.pid
export FLAMEHAZE_LOG_DIR=/var/log
export FLAMEHAZE_USER=flamehaze

export PATH=$PATH:/usr/bin
